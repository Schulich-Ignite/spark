    "%%ignite\n",
    "\n",
    "# Unhappy path\n",
    "#  Incorrect number of args is rejected\n",
    "#  Non-ints are rejected for RGB\n",
    "#  None as arg is rejected\n",
    "\n",
    "def setup():\n",
    "    print(\"fill_style throws exceptions\")\n",
    "    size(100, 100)\n",
    "    expect_type_error(with_missing_args, \"fill_style expected 1, 3, or 4 arguments, got 0\")\n",
    "    expect_type_error(with_none_in_rgba, \"Invalid types for fill_style with 4 arguments, expected \\n\\tfill_style(int, int, int, Number)\\nreceived fill_style(None: NoneType, None: NoneType, None: NoneType, None: NoneType)\")\n",
    "    expect_type_error(with_string_in_rgb, \"Invalid types for fill_style with 3 arguments, expected \\n\\tfill_style(int, int, int)\\nreceived fill_style(x: str, y: str, z: str)\")\n",
    "    expect_type_error(with_float_in_rgb, \"Invalid types for fill_style with 3 arguments, expected \\n\\tfill_style(int, int, int)\\nreceived fill_style(128.0: float, 128: int, 128: int)\")\n",
    "    expect_type_error(with_none_in_string, \"Invalid types for fill_style with 1 argument, expected one of \\n\\tfill_style(str)\\n\\tfill_style(int)\\nreceived fill_style(None: NoneType)\")\n",
    "    \n",
    "def expect_type_error(func, expected_error):\n",
    "    try:\n",
    "        func()\n",
    "    except Exception as e:\n",
    "        if str(e) != expected_error:\n",
    "            print(\"FAIL:\\n\\tExpected \" + str(func.__name__) + \" to raise error:\\n\\t\\t\" + expected_error)\n",
    "            print(\"\\tbut received:\\n\\t\\t\" + str(e).replace(\"\\n\",\"\\n\\t\\t\"))\n",
    "            \n",
    "def with_missing_args():\n",
    "    print(\"with missing args\")\n",
    "    fill_style()\n",
    "    \n",
    "def with_none_in_string():\n",
    "    print(\"with None in string\")\n",
    "    fill_style(None)\n",
    "    \n",
    "def with_none_in_rgba():\n",
    "    print(\"with None-types in rgba\")\n",
    "    fill_style(None, None, None, None)\n",
    "    \n",
    "def with_string_in_rgb():\n",
    "    print(\"with string in rgb\")\n",
    "    fill_style('x', 'y', 'z')\n",
    "    \n",
    "def with_float_in_rgb():\n",
    "    print(\"with float in rgb\")\n",
    "    fill_style(128.0, 128, 128)"
   ]
  },
